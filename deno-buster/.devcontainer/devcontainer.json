{
  "name": "Deno - Devcontainer",
  "image": "artiefuzzz/codespace:deno",
  "extensions": [
    "aaron-bond.better-comments",
    "dbaeumer.vscode-eslint",
    "editorconfig.editorconfig",
    "eamodio.gitlens",
    "quicktype.quicktype",
    "christian-kohler.path-intellisense",
    "esbenp.prettier-vscode"
  ],
  "settings": {
    "terminal.integrated.defaultProfile.linux": "zsh",
    "files.eol": "auto",
    "editor.formatOnSave": true,
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": true,
    "terminal.integrated.cursorBlinking": true,
    "window.titleSeparator": " | ",
    "window.title": "${dirty}${activeEditorShort}${separator}${activeFolderShort}",
    "terminal.integrated.tabs.title": "${process}${separator}${workspaceFolder}",
    "editor.tabSize": 2,
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs": "active",
    "editor.integrated.fontFamily": "JetBrains Mono",
    "javascript.updateImportsOnFileMove.enabled": "always"
  }
}
